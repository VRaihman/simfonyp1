<script src='static/moment.min.js'></script>
<script src='static/jquery.min.js'></script>
<script src='static/fullcalendar.min.js'></script>
<script>
$(document).ready(function() {
	
	$.ajax({
        url: "ajax/getEvents",
        type: "POST",
        data: 'date=2000-00-20',
        dataType: 'json',
        success: function(ee) {
			console.log(ee.days);
			$('#calendar').fullCalendar({
				events: ee.days,
			});

		}
	});

});

</script>
